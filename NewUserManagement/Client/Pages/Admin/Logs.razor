@* @page "/logs"
@using YourApplication.Services // Update with your service namespace

<h3>Log Entries</h3>

@if (LogEntries == null)
{
	<p>Loading...</p>
}
else if (LogEntries.Count == 0)
{
	<p>No log entries found.</p>
}
else
{
	<ul>
		@foreach (var logEntry in LogEntries)
		{
			<li>@logEntry.Timestamp - @logEntry.Message</li>
		}
	</ul>
}

@code {
	List<LogEntry> LogEntries { get; set; }

	protected override async Task OnInitializedAsync()
	{
		// Fetch log entries from the service
		LogEntries = await LogService.GetLogEntriesAsync();
	}
} *@
